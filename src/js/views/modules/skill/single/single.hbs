<div class="media">
	{{> ../partials/icon}}
	<div class="media-body">
		<h3 class="mt-3">{{name}} <span class="pjs__words">{{{words words}}}</span></h3>
		<p class="text-muted">Clase {{type}}</p>
	</div>
</div>

{{#if skill_id}}
	<p class="skill__requirement">
		<strong>Requisito:</strong> <a class="action__requirement" tabindex="-1">{{parentName}}</a>
	</p>
{{/if}}

{{#if letany}}
	<p class="skill__letany"><strong>Letan&iacute;a:</strong> <em>{{letany}}</em></p>
{{/if}}

<div class="description">
	{{{md description}}}
</div>



</div>

<h3 class="body__subsection">Disponible para:</h3>
<div class="classes__region"></div>
